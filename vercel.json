{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "public",
  "installCommand": "npm install",
  "routes": [
    {
      "src": "/",
      "dest": "/home.html"
    },
    {
      "src": "/chat",
      "dest": "/index.html"
    },
    {
      "src": "/expert",
      "dest": "/expert.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
